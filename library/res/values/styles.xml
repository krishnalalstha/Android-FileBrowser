<resources>

    <style name="ListbingoTheme" parent="@android:style/Theme.Light">
     <!--    <item name="android:buttonStyle">@style/CustomButtonStyle</item> -->
       
      
       
        
       
       
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowBackground">@null</item>
       
    </style>

   

   

   

    <style name="listItemTextStyle">
        <item name="android:textColor">#545353</item>
        <item name="android:textStyle">bold</item>
        <item name="android:shadowColor">#FFFFFF</item>
        <item name="android:shadowDx">0</item>
        <item name="android:shadowDy">1</item>
        <item name="android:shadowRadius">0.01</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:layout_centerVertical">true</item>
    </style>

    <style name="listItemDescTextStyle">
        <item name="android:textColor">#757575</item>
        <item name="android:textSize">14dip</item>
        <item name="android:shadowColor">#FFFFFF</item>
        <item name="android:shadowDx">0</item>
        <item name="android:shadowDy">1</item>
        <item name="android:shadowRadius">0.01</item>
    </style>

    <style name="MapOverLayItemTitleTextStyle">
        <item name="android:textSize">14dip</item>
        <item name="android:textColor">#FFFFFF</item>
        <item name="android:textStyle">bold</item>
        <item name="android:shadowColor">#2f2e2e</item>
        <item name="android:shadowRadius">0.01</item>
        <item name="android:shadowDx">0</item>
        <item name="android:shadowDy">1</item>
    </style>

    <style name="CurrentPageInfoStyle">
        <item name="android:background">@color/header_bgcolor</item>
        <item name="android:textColor">#FFFFFF</item>
        <item name="android:textSize">15dip</item>
        <item name="android:textStyle">bold</item>
        <item name="android:padding">5dip</item>
    </style>

